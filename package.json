{
  "name": "spsave",
  "description": "Save files in SharePoint using node.js easily",
  "version": "4.0.1",
  "author": "Sergei Sergeev <sergeev.srg@gmail.com>",
  "main": "./lib/src/index.js",
  "typings": "./lib/src/index",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc.json --ext .ts src test",
    "build": "npm run lint && tsc -p .",
    "prepublishOnly": "rimraf -- lib && npm run build",
    "test:unit": "mocha ./test/unit/tests.ts --watch --watch-extensions ts",
    "coverage": "nyc mocha ./test/integration/tests.ts",
    "test:dev": "mocha ./test/integration/tests.ts --watch --watch-extensions ts"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "report-dir": "coverage/integration",
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "full-trace": true,
    "bail": true
  },
  "bugs": {
    "url": "https://github.com/s-KaiNet/spsave/issues"
  },
  "keywords": [
    "request",
    "sharepoint",
    "rest",
    "save file"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/s-KaiNet/spsave.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/mockery": "^1.4.29",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "chai": "^4.2.0",
    "console.table": "^0.10.0",
    "eslint": "^7.3.0",
    "map-stream": "0.0.7",
    "mocha": "^8.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "mockery": "^2.1.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "vinyl-fs": "^3.0.3"
  },
  "dependencies": {
    "@types/core-js": "^2.5.3",
    "@types/node": "^14.0.13",
    "@types/node-notifier": "6.0.1",
    "@types/vinyl": "^2.0.4",
    "@types/vinyl-fs": "2.4.11",
    "colors": "1.4.0",
    "globby": "^6.1.0",
    "node-notifier": "7.0.1",
    "sp-request": "^3.0.0",
    "vinyl": "2.2.0"
  }
}
