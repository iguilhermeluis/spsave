{
  "name": "spsave",
  "description": "Save files in SharePoint using node.js easily",
  "version": "4.0.0-beta.0",
  "author": "Sergei Sergeev <sergeev.srg@gmail.com>",
  "main": "./lib/src/index.js",
  "typings": "./lib/src/index",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "gulp test-ci",
    "dev": "gulp live-dev",
    "test-int": "gulp test-int",
    "test-perf": "gulp tsc && node lib/test/performance/index.js",
    "lint": "eslint -c .eslintrc.json --ext .ts src test",
    "build": "npm run lint && tsc -p .",
    "prepublishOnly": "rimraf -- lib && npm run build"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "report-dir": "coverage/integration",
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "full-trace": true,
    "bail": true
  },
  "bugs": {
    "url": "https://github.com/s-KaiNet/spsave/issues"
  },
  "keywords": [
    "request",
    "sharepoint",
    "rest",
    "save file"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/s-KaiNet/spsave.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/mockery": "^1.4.29",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "chai": "^4.2.0",
    "console.table": "^0.10.0",
    "del": "^5.1.0",
    "eslint": "^7.1.0",
    "mocha-junit-reporter": "^1.23.3",
    "mockery": "^2.1.0",
    "remap-istanbul": "^0.13.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "typescript": "^3.9.3",
    "vinyl-fs": "^3.0.3"
  },
  "dependencies": {
    "@types/core-js": "^2.5.3",
    "@types/lodash": "^4.14.152",
    "@types/node": "^14.0.5",
    "@types/node-notifier": "6.0.1",
    "@types/vinyl": "^2.0.4",
    "@types/vinyl-fs": "2.4.11",
    "colors": "1.4.0",
    "globby": "11.0.0",
    "node-notifier": "7.0.1",
    "sp-request": "^3.0.0-beta.2",
    "vinyl": "2.2.0"
  }
}
